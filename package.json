    "expo-device": "~6.0.2",
    "expo-notifications": "~0.28.17",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
